<!DOCTYPE html>
<html>
<head>
  <title>Customer Data</title>
  <style>
    table {
      border-collapse: collapse;
      width: 100%;
    }

    th, td {
      text-align: left;
      padding: 8px;
    }

    tr:nth-child(even) {
      background-color: #f2f2f2;
    }
  </style>
</head>
<body>
  <h1>Customer Data</h1>
  <table>
    <thead>
      <tr>
        <th>Company Name</th>
        <th>Contact Name</th>
        <th>City</th>
        <th>Country</th>
      </tr>
    </thead>
    <tbody>
      <% for (var i = 0; i < data.length; i++) { %>
        <tr>
          <td><%= data[i].CompanyName %></td>
          <td><%= data[i].ContactName %></td>
          <td><%= data[i].City %></td>
          <td><%= data[i].Country %></td>
        </tr>
      <% } %>
    </tbody>
  </table>
</body>
</html>
